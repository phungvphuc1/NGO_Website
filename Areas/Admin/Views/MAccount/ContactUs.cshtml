@model IEnumerable<NGOWebApp.Models.ContactUs>
@{
    ViewData["Title"] = "ContactUs";
}

<h1>ContactUs</h1>


    <table id="example">

        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(m => m.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Phone)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Content)
                </th>



            </tr>
            

        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(i => item.Id)</td>
                    <td>@Html.DisplayFor(i => item.Name)</td>
                    <td>@Html.DisplayFor(i => item.Email)</td>
                    <td>@Html.DisplayFor(i => item.Phone)</td>
                    <td>@Html.DisplayFor(i => item.Content)</td>

                </tr>
            }
        </tbody>
    </table>


<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts{



    <script>
$(document).ready(function () {
            $('#example').DataTable();
        });</script>

}